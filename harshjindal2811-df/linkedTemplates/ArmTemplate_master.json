{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "harshjindal2811-df"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureDataLakeStorageForCustomer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageForCustomer'"
		},
		"SqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureprojectharsh.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageForCustomer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureprojectharsh.dfs.core.windows.net/"
		},
		"RestCountriesAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/name/"
		},
		"SqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "HARSHJINDAL\\SQLEXPRESS"
		},
		"SqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "harsh"
		},
		"SqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Harsh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "harshjindal2811-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureDataLakeStorageForCustomer_accountKey": {
						"value": "[parameters('AzureDataLakeStorageForCustomer_accountKey')]"
					},
					"SqlServer_password": {
						"value": "[parameters('SqlServer_password')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageForCustomer_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageForCustomer_properties_typeProperties_url')]"
					},
					"RestCountriesAPI_properties_typeProperties_url": {
						"value": "[parameters('RestCountriesAPI_properties_typeProperties_url')]"
					},
					"SqlServer_properties_typeProperties_server": {
						"value": "[parameters('SqlServer_properties_typeProperties_server')]"
					},
					"SqlServer_properties_typeProperties_database": {
						"value": "[parameters('SqlServer_properties_typeProperties_database')]"
					},
					"SqlServer_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}